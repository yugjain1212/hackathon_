rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    match /public-events.json {
      allow read;
      allow write: if request.auth != null && request.auth.token.admin == true;
    }
    match /{allPaths=**} {
      allow read: if false;
      allow write: if false;
    }
  }
}
